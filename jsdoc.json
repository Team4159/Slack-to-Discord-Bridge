{
	"source": {
		"include": [],
		"includePattern": ".+\\.js$",
		"exclude": [
			"node_modules",
			"docs",
			".git",
			"testing",
			"setup"
		]
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"encoding": "utf8",
		"readme": "./README.md",
		"destination": "docs/",
		"template": "node_modules/docdash",
		"recurse": true
	},
	"docdash": {
		"sort": true,
		"search": true
	}
}
